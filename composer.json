{
    "name": "yakeing/php_badge",
    "description": "This is an identification tag based on SVG.",
    "keywords": ["Badge", "SVG"],
    "homepage": "https://github.com/yakeing/php_badge",
    "type": "symfony-bundle",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "yakeing",
            "homepage": "http://weibo.com/yakeing"
        }
    ],
    "autoload": {
        "psr-4": {
            "php_badge\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
         "phpunit/phpunit": "5.7"
    },
    "extra":{
        "branch-alias":{
            "dev-master": "2.x-dev"
        }
    }
}
